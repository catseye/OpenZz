!!REFERENCE OUTPUT
!!registered foo tag (ret=1)
!!foo_new called
!!invoked param ctor (tag=foo name=c)
!!tag_of(c)= foo
!!invoked param dtor (tag=foo name=c)
!!foo_remove_tag called
!!END OUTPUT

/aa=/load_lib ".libs/libtagdtor.so"

!!/print "lib handle=", $zz$hexify(aa)

/stat-> "bubu" {
!!/print $foo_new
/c= $foo_new

/print "tag_of(c)=",tag_of(c)
}

bubu

$foo_remove_tag
